{
    "name": "gruz/fusionpbx-api",
    "description": "A Laravel based FusionPBX API",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "AHeavyObject",
        "email": "vongruz@protonmail.com"
    }],
    "autoload": {
        "psr-4": {
            "Gruz\\FPBX\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gruz\\FPBX\\Tests\\": "tests/",
            "Gruz\\FPBX\\Database\\Factories\\": "database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gruz\\FPBX\\Providers\\FPBXAppServiceProvider",
                "Gruz\\FPBX\\Providers\\FPBXEventServiceProvider",
                "Gruz\\FPBX\\Providers\\FPBXHelperServiceProvider",
                "Gruz\\FPBX\\Providers\\FPBXRouteServiceProvider"
            ]
        }
    },
    "require": {
        "awobaz/compoships": "^2.1",
        "laravel/sanctum": "^2.11",
        "doctrine/annotations": "^1.12",
        "optimus/api-consumer": "^0.4.0",
        "optimus/architect": "^2.0",
        "gruz/bruno": "^6.0",
        "gruz/genie": "^3.3",
        "optimus/heimdal": "^3.0",
        "darkaonline/l5-swagger": "~8.1",
        "doctrine/dbal": "^3.1",
        "laravel/ui": "^3.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.19",
        "phpunit/phpunit": "^9.5",
        "kint-php/kint": "^3.3"
    }
}
