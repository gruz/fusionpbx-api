{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.3.0",
        "laravel/passport": "^10.1",
        "optimus/heimdal": "^3.0",
        "optimus/distributed-laravel": "^1.0",
        "laravel/framework": "^8.33",
        "optimus/bruno": "^6.0",
        "optimus/genie": "^3.3",
        "optimus/api-consumer": "^0.4.0",
        "optimus/architect": "^2.0",
        "gruz/laravel-ratchet": "^8.0",
        "darkaonline/l5-swagger": "^8.0",
        "doctrine/annotations": "^1.12",
        "laravel/tinker": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Infrastructure\\": "infrastructure/",
            "Api\\": "api/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "../fusionpbx/resources/functions.php",
            "../fusionpbx/resources/classes/message.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:gruz/L5-Swagger.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "kint-php/kint": "^3.3",
        "nunomaduro/collision": "^5.3",
        "filp/whoops": "^2.11",
        "beyondcode/laravel-dump-server": "^1.7",
        "mockery/mockery": "^1.4",
        "fakerphp/faker": "^1.13",
        "barryvdh/laravel-ide-helper": "^2.9",
        "barryvdh/laravel-debugbar": "^3.5"
    }
}
