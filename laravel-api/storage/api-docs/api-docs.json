{
    "openapi": "3.0.0",
    "info": {
        "title": "FusionPBX API",
        "contact": {
            "email": "vongruz@protonmail.com"
        },
        "version": "0.0.8-83-g2802bf0"
    },
    "paths": {
        "/signup/domain": {
            "post": {
                "tags": [
                    "Domain",
                    "User"
                ],
                "summary": "Create a domain",
                "description": "Creates a domain. Depending on the configuration the domain `domain_name` can be a subdomain of the main domain or an independent domain",
                "parameters": [
                    {
                        "name": "domain_uuid",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    },
                    {
                        "name": "domain_name",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    },
                    {
                        "name": "domain_enabled",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    },
                    {
                        "name": "domain_description",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    },
                    {
                        "name": "domain_parent_uuid",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Client side search object",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "allOf": [
                                    {
                                        "$ref": "#/components/schemas/Domain"
                                    },
                                    {
                                        "$ref": "#/components/schemas/User"
                                    },
                                    {
                                        "properties": {
                                            "iidd": {
                                                "type": "integer"
                                            },
                                            "created_at": {
                                                "$ref": "#/components/schemas/Domain/properties/domain_uuid"
                                            }
                                        },
                                        "type": "object"
                                    }
                                ],
                                "x-model-input-fields": [
                                    "#/components/schemas/Domain",
                                    "#/components/schemas/User"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Get application name and version",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/Domain"
                                        },
                                        {
                                            "properties": {
                                                "iidd": {
                                                    "type": "integer"
                                                },
                                                "created_at": {
                                                    "$ref": "#/components/schemas/Domain/properties/domain_uuid"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
        "/": {
            "get": {
                "tags": [
                    "Default"
                ],
                "description": "Get application name and version",
                "operationId": "Infrastructure\\Api\\Controllers\\DefaultApiController::index",
                "responses": {
                    "200": {
                        "description": "Application name and version",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "title": {
                                            "description": "APP_NAME variable",
                                            "type": "string"
                                        },
                                        "version": {
                                            "description": "Fit tag version",
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                },
                                "examples": [
                                    {
                                        "example": "Asdsads",
                                        "summary": "App info",
                                        "value": {
                                            "title": "FusionPBX API",
                                            "version": "0.0.8-62-g192d97b"
                                        }
                                    }
                                ]
                            }
                        }
                    }
                },
                "security": []
            }
        }
    },
    "components": {
        "schemas": {
            "Domain": {
                "properties": {
                    "domain_uuid": {
                        "schema": "domain_uuid",
                        "description": "Domain id",
                        "readOnly": true,
                        "example": "54cd6070-3b0d-11e7-bf5a-4be762d404ce"
                    },
                    "domain_name": {
                        "description": "Domain or subdomain name",
                        "type": "string",
                        "example": "site.com"
                    },
                    "domain_enabled": {
                        "description": "If domain is active",
                        "type": "boolean",
                        "example": false
                    },
                    "domain_description": {
                        "description": "Domain desctiption. Automatically generated upon domain creation if nothing passed",
                        "type": "string",
                        "example": "Created via api at 2017-05-17 14:30:10"
                    },
                    "domain_parent_uuid": {
                        "description": "Parant domain id",
                        "example": "54cdc4b0-3b0d-11e7-888f-c38f274a1cd2",
                        "nullable": true,
                        "x-final": true
                    }
                },
                "type": "object"
            },
            "User": {}
        },
        "securitySchemes": {
            "api_key": {
                "type": "apiKey",
                "name": "api_key",
                "in": "header"
            }
        }
    },
    "tags": [
        {
            "name": "Default"
        },
        {
            "name": "Domain",
            "description": "Manage domains"
        },
        {
            "name": "User",
            "description": "Manage users"
        },
        {
            "name": "Extension",
            "description": "Manage extensions"
        }
    ],
    "security": [
        []
    ]
}